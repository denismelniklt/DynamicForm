@model FormViewModel

<div>
    <table>
        <tr>
            <td colspan="2">
                @Model.Name
            </td>
        </tr>
        <tr>
            <td>
                Registracijos požymiai
            </td>
            <td>
                Reikšmė
            </td>
        </tr>
       
            @foreach (var property in Model.Properties)
            {
                <tr>
                    <td>
                        @property.Name
                    </td>
                    <td>
                        @Html.DropDownList(
                            "Values",
                            new SelectList(property.Values.Select(v => v.Name))
                        )
                    )
                    </td>
                </tr>
            }
                
    </table>
</div>